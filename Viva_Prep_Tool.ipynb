        "import gradio as gr\n",
        "import chromadb\n",
        "import PyPDF2\n",
        "from dotenv import load_dotenv\n",
        "import os\n",
        "# import replicate\n",
        "from groq import Groq\n",
        "from google.colab import userdata"
      ],
      "metadata": {
